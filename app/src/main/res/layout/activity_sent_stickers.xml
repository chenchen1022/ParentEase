<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SentStickersActivity">

    <ImageView
        android:id="@+id/weather_icon_clear"
        android:layout_width="131dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/weather_icon_clear"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="90dp" />

    <TextView
        android:id="@+id/weather_icon_clear_sent_times"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sent times: "
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageView
        android:id="@+id/weather_icon_clouds"
        android:layout_width="131dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/weather_icon_clouds"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="90dp" />

    <TextView
        android:id="@+id/weather_icon_clouds_sent_times"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sent times: "
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="192dp" />

    <ImageView
        android:id="@+id/weather_icon_rainbow"
        android:layout_width="131dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/weather_icon_rainbow"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="356dp" />

    <ImageView
        android:id="@+id/weather_icon_snow"
        android:layout_width="131dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/weather_icon_snow"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="494dp" />

    <ImageView
        android:id="@+id/weather_icon_bolt"
        android:layout_width="131dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/weather_icon_bolt"
